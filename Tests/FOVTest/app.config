<?xml version="1.0"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2"/>
	</startup>

	<system.diagnostics>
		<trace autoflush="true">
			<listeners>
				<add name="clientTraceListener"/>
				<remove name="Default"/>
			</listeners>
		</trace>

		<sharedListeners>
			<add name="clientTraceListener" type="Dwarrowdelf.MMLogTraceListener,Dwarrowdelf.Common"/>
		</sharedListeners>
	</system.diagnostics>

</configuration>
